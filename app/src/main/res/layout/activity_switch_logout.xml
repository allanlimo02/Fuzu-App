<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fbf3ee"
    tools:context=".SwitchAccount.SwitchLogoutActivity">
    <include
        android:id="@+id/include"
        layout="@layout/toolbar" />

    <RelativeLayout
        android:id="@+id/relativeone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:paddingStart="10dp"
        android:paddingBottom="10dp"
        android:layout_marginStart="221dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="#fbf3ee"
        app:layout_constraintTop_toBottomOf="@+id/include">

        <TextView
            android:id="@+id/backswitch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:paddingEnd="10dp"
            android:text="Switch Account"
            android:textColor="#004673"
            android:textStyle="bold"
            app:drawableEndCompat="@drawable/ic_arrow_right" />

        <TextView
            android:id="@+id/logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/backswitch"
            android:layout_marginTop="5dp"
            android:text="log Out"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            app:drawableEndCompat="@drawable/ic_arrow_right" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>